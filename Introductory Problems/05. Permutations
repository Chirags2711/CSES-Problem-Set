private static void solve(long n) {
    if(n >=2 && n <= 3){
        System.out.println("NO SOLUTION");
        return;
    }
    StringBuilder sb = new StringBuilder();
    for(long i=2; i<=n; i+=2){
        sb.append(i);
        sb.append(' ');
    }
    for(long i=1; i<=n; i+=2){
        sb.append(i);
        sb.append(' ');
    }
    System.out.println(sb);
}
